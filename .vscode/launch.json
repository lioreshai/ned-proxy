{
    "version": "0.2.0",
    "configurations": [
    {
        "type": "node",
        "request": "launch",
        "name": "nodemon",
        "runtimeExecutable": "${workspaceRoot}/node_modules/nodemon/bin/nodemon.js",
        "args": ["--watch", "dist/"],
        "runtimeArgs": ["-r","source-map-support/register","--nolazy","--trace-deprecation","--trace-warnings"],
        "outFiles": ["${workspaceRoot}/src"],
        "sourceMaps": true,
        "console": "integratedTerminal"
    }
    ]
}